<table class="table">
  <thead>
    <tr>
      <th>Event Name</th>
      <th>Start at</th>
      <th>Edit Event</th>
      <th>Ticket types</th>
      <th>Add ticket type</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |e| %>
      <% if e.user_id == current_user.id %>
      <tr>
        <td><%= e.name %></td>  
        <td><%= e.starts_at %></td>  
        <td><%= link_to "Edit event", edit_event_path(e) %></td>  
        <td>
          <% e.ticket_types.each do |t| %>
            <%= e.name %>,
          <% end %>
        </td>
        <td>
          <%= link_to "Add new ticket_type", new_ticket_type_path(event: e.id) %>
        </td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
